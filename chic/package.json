{
  "author": "studioKeywi <dev@studiokeywi.dev> (https://studiokeywi.dev)",
  "browser": "./dist/index.js",
  "description": "Easily apply CSS styles to your browser console output with Chic!",
  "devDependencies": {
    "esbuild": "^0.16.17",
    "typescript": "^4.9.4"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugins": {
      "types": "./types/plugins.d.ts",
      "import": "./dist/plugins.js"
    },
    "./plugins/*": {
      "types": "./types/plugins/*.d.ts",
      "import": "./dist/plugins/*.js"
    }
  },
  "keywords": [],
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@studiokeywi/chic",
  "repository": {
    "directory": "chic",
    "type": "git",
    "url": "github:studiokeywi/chic"
  },
  "scripts": {
    "build": "node build.js & tsc",
    "prebuild:clean": "npm run clean",
    "build:clean": "npm run build",
    "clean": "rm -rf ./dist & rm -rf ./types",
    "prepublish": "npm run build:clean"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./types/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "plugins": [
        "./types/plugins/index.d.ts"
      ],
      "plugins/*": [
        "./types/plugins/*.d.ts"
      ]
    }
  },
  "version": "0.4.6"
}
