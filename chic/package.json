{
  "name": "@studiokeywi/chic",
  "version": "0.4.4",
  "description": "Browser console.log CSS helper",
  "keywords": [],
  "author": "studioKeywi <dev@studiokeywi.dev> (https://studiokeywi.dev)",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./build": {
      "types": "./types/chic.d.ts",
      "import": "./dist/chic.js"
    },
    "./plugins": {
      "types": "./types/plugins/index.d.ts",
      "import": "./dist/plugins/index.js"
    },
    "./plugins/*": {
      "types": "./types/plugins/*.d.ts",
      "import": "./dist/plugins/*.js"
    }
  },
  "types": "./types/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "build": [
        "./types/chic.d.ts"
      ],
      "plugins": [
        "./types/plugins/index.d.ts"
      ],
      "plugins/*": [
        "./types/plugins/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "node build.js & tsc",
    "prebuild:clean": "npm run clean",
    "build:clean": "npm run build",
    "clean": "rm -rf ./dist & rm -rf ./types",
    "prepublish": "npm run build:clean"
  },
  "repository": {
    "type": "git",
    "url": "github:studiokeywi/chic"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "esbuild": "^0.16.17",
    "typescript": "^4.9.4"
  }
}
